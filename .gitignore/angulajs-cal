	<div ng-app="myapp" ng-controller="myCtrl" ng-init="mathin='0'">
 <input type="text" ng-model="mathin">
<p>{{fun(mathin)}}</p>

	</div>

	<script>
	var app = angular.module("myapp", []);
	app.controller("myCtrl", function($scope) {
	 $scope.fun=function(v){return eval(v)};
		 
	   
	});
	</script>
